    <script>
        $(function f(){
        f.count_event_id = 1;
        $(".test1").on("click", function () {
        var temp_count = f.count_event_id+1;
        if (temp_count <= 3){
        f.count_event_id++;
        var dup = $("#1").clone();
        dup.attr('id', f.count_event_id);
        dup.appendTo(".events");
        divparent = dup.closest('div');
        divparent.find(".cancelevent1").show();
        intext = divparent.find(".dest");;
        intext.val(eval(intext.attr("onblur")));
        if (f.count_event_id > 2) {
        $(".test1").hide();
        }
        }
        });

        $(document).on("click",".cancelevent1", function () {
        f.count_event_id--;
        if (f.count_event_id < 3) {
        $(".test1").show();
        }
        $(this).closest('div').remove();
        });
        })
